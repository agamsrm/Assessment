package com.assessment.ProjectCRUD.Entity;

import javax.persistence.*;

import lombok.*;

@Getter
@Setter
@Data
@Entity

public class Product {

	@Id
	@GeneratedValue(strategy = GenerationType.IDENTITY)
	long id;
	@Column
	String product_name;
	@Column
	String product_description;
	@Column
	long price;

}
